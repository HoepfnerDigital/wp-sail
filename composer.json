{
    "name": "hoepfner-digital/wp-sail",
    "description": "Docker files for running a basic WordPress application. Derived from Wordpress Sail",
    "keywords": ["wordpress", "docker"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/hoepfner-digital/wp-sail/issues",
        "source": "https://github.com/hoepfner-digital/wp-sail"
    },
    "authors": [
        {
            "name": "Tim Hoepfner",
            "email": "tim@hoepfnerdigital.de"
        },
        {
            "name": "Ethan Clevenger",
            "email": "ethan@sternerstuff.dev"
        },
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/contracts": "^8.0|^9.0",
        "illuminate/support": "^8.0|^9.0",
        "wp-cli/wp-cli": "^2.7"
    },
    "bin": [
        "bin/sail"
    ],
    "autoload": {
        "psr-4": {
            "HoepfnerDigital\\WpSail\\": "src/"
        },
        "files": [
            "add_commands.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
